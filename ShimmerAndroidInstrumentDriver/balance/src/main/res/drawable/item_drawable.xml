<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#E9EFFF"/>
            <size  android:height="60dp"/>
            <corners android:radius="16dp"/>
        </shape>
    </item>

    <item
        android:left="10dp"
        android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FBFBFF"/>
            <corners android:radius="16dp"/>
        </shape>
    </item>

</layer-list>
